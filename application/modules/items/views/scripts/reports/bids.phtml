
<div class="row">
    <div class="col-sm">
        <canvas id="bidChart"></canvas>
        <script>
            $(function() {
                console.log('Before');
                $.getJSON('http://system.metrotorontorotaryauction.com/items/reports/bidsdata', function(data) {
                    console.log('Hello');
                    let blocks = [], bidCount = [];

                    console.log(data);

                    $.each(data, function(key, val) {
                        blocks.push([val.block, val.time]);
                        bidCount.push(val.bidcount);

                    });

                    console.log(blocks);

                    let ctx = document.getElementById('bidChart').getContext('2d');
                    let bidChart = new Chart(ctx, {
                        type: 'bar',
                        data: {
                            labels: blocks,
                            datasets: [{
                                label: '# of bids',
                                data: bidCount,
                                borderColor: '#01C0C8',
                                pointBackgroundColor: '#01c0c8'
                            }]
                        },
                        options: {
                            responsive: true,
                            title: {
                                display: true,
                                text: 'Bids by Block'
                            },
                            scales: {
                                yAxes: [{
                                    ticks: {
                                        beginAtZero: true
                                    }
                                }]
                            }
                        }
                    });
                });
            });
        </script>
    </div>
</div>