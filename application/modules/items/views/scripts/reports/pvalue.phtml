<div class="row">
    <div class="col-sm">
        <canvas id="percentChart"></canvas>
        <script>
            $(function() {
                console.log('Before');
                $.getJSON('http://system.metrotorontorotaryauction.com/items/reports/pvaluedata', function(data) {
                    let blocks = [], percents = [];

                    $.each(data, function(key, val) {
                        blocks.push([val.block, val.time]);
                        percents.push(val.percents);
                    });

                    let ctx = document.getElementById('percentChart').getContext('2d');
                    let bidChart = new Chart(ctx, {
                        type: 'bar',
                        data: {
                            labels: blocks,
                            datasets: [{
                                label: '% of Value',
                                data: percents,
                                borderColor: 'rgb(54, 162, 235)',
                                backgroundColor: 'rgba(54,162,235,0.2)',
                                borderWidth: 1
                            }]
                        },
                        options: {
                            responsive: true,
                            title: {
                                display: true,
                                text: 'Percent of Value by Block'
                            },
                            scales: {
                                yAxes: [{
                                    ticks: {
                                        beginAtZero: true
                                    }
                                }]
                            }
                        }
                    });
                });
            });
        </script>
    </div>
</div>